{
	"name": "easycatalog_ecc",
	"displayName": "Easycatalog Language",
	"description": "Easycatalog language extension and snippets",
	"version": "0.0.1",
	"publisher": "julux",
	"engines": {
		"vscode": "^1.28.0"
	},
	"galleryBanner": {
		"color": "#1E81C1",
		"theme": "light"
	},	
	"categories": [
		"Snippets",
		"Programming Languages"
	],
	"contributes": {
		"languages": [
				{
			  	"id": "easycatalog",
			 	 "extensions": [
						".ecc"
			  			],
			 	 "aliases": [
						"ecc",
						"Easycatalog"
			  			],
			  	"configuration": "./language-configuration.json"
				}
		 		 ],
		"snippets": 	[
				{
				"language": "easycatalog",
				"path": "./snippets/snippets-easycatalog.json"
				}
				]
			},
	"license": "MIT",
	"bugs": {
			"url": "https://github.com/julux/vscode-easycatalog/issues"
	},
	"repository": {
			"type": "git",
			"url": "https://github.com/julux/vscode-easycatalog.git"
	},
	"homepage": "https://github.com/julux/vscode-easycatalog/blob/master/README.md"
	
	
}
